{
    "name": "benharri/benbot",
    "description": "general purpose discord bot",
    "type": "discord bot",
    "authors": [
        {
            "name": "Ben Harris",
            "email": "mail@benharris.ch"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "team-reflex/discord-php": "dev-develop",
        "guzzlehttp/guzzle": "~6.0",
        "vlucas/phpdotenv": "^2.4",
        "voku/stringy": "^2.2",
        "ext-sodium": "*",
        "ext-msgpack": "*",
        "php": ">=7.1.4"
    },
    "autoload": {
        "psr-4": {
            "BenBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BenBot\\Tests\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    }
}
